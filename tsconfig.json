{
  "include": ["src/app/**/*"],
  "compilerOptions": {
    "target": "es5",
    "module": "commonJs",
    "outDir": "./app",
    "strict": true,
    "esModuleInterop": true,
    "sourceMap": true,
    "baseUrl": "./",
    "jsx": "react",
    "lib": ["ES5", "ES6", "DOM"],
    "allowSyntheticDefaultImports": true,
    "isolatedModules": true,
    "skipLibCheck": true,
  },
  "exclude": [
    "node_modules"
  ]
}
